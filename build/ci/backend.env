###
### Backend
###
# Уровень логирования (debug, info, warn, error, fatal)
#TN_LOGLEVEL=warn
# Формат вывода логов (console/json)
#TN_LOG_OUTPUT=json
# Ip, на котором запускается сервер
#TN_IP=0.0.0.0
# Порт для grpc сервера
#TN_GRPC_PORT=8080
# Порт для http сервера
#TN_HTTP_PORT=3015

###
### Postgres
###
# DSN для доступа к postgres
TN_POSTGRES_DSN=user=postgres password=nick host=postgres port=5432 database=postgres sslmode=disable
# Путь к папке с миграциями
TN_MIGRATION_MIGRATIONS=file://cmd/be/migrations
# Требуемая версия миграций
#TN_MIGRATION_VERSION_TABLE=public.schema_version

###
### Profile
###
# URL API ЦП
TN_PROFILE_EMPLOYEE_API_URL=
# API token (employee)
TN_PROFILE_EMPLOYEE_API_TOKEN=
# Client ID в ЦП
TN_PROFILE_CLIENT_ID=
# Client secret в ЦП
TN_PROFILE_CLIENT_SECRET=
# URL для авторизации пользователя
TN_PROFILE_AUTH_URL=
# URL для получения токена
TN_PROFILE_TOKEN_URL=
# OAuth redirect url
TN_PROFILE_REDIRECT_URL=
# Необходимые claims для работы приложения
#TN_PROFILE_SCOPES=phone.read,employee.read,email.read

###
### Sessions
###
# Время жизни access токена
#TN_ACCESS_EXPIRY=1h
# Время жизни refresh токена
#TN_REFRESH_EXPIRY=48h
# Secret для подписи токена
TN_SECRET=


###
### Email sender
###
# Флаг на проверку TLS
#TN_SKIP_TLS=false
# Хост SMTP-сервера
#TN_EMAIL_HOST=
# Порт SMTP-сервера
#TN_EMAIL_PORT=
# Пользователь SMTP-сервера
#TN_EMAIL_USER=
# Пароль пользователя SMTP-сервера
#TN_EMAIL_PASSWORD=
# Системный email
#TN_EMAIL_FROM=tn.booking.app@gmail.com

###
